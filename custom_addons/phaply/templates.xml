				<form string="Doi tac">
					<sheet>
						<group>
							<field name="name"/>
						</group>
					</sheet>
				</form>
			</field>
		</record>
		
		<record model="ir.ui.view" id="phaply_doitac_tree">
			<field name="name">Doi tac KH Tree</field>
			<field name="model">phaply.doitac</field>
			<field name="arch" type="xml">
				<tree string="Doi tac">
					<field name="name"/>
				</tree>
			</field>
		</record>
		
		<record model="ir.actions.act_window" id="phaply_doitac_action">
			<field name="name">Doi tac</field>
			<field name="res_model">phaply.doitac</field>
			<field name="view_mode">tree,form</field>
		</record>
		
		<menuitem name="Doi tac" id="phaply_doitac_menu" parent="phaply_tuychinh_menu" sequence="20" action="phaply_doitac_action"/>
    </data>
</openerp>
<openerp>
    <data>
    	<record model="ir.ui.view" id="service_form_view">
    		<field name="name">Dich vu</field>
    		<field name="model">phaply.service</field>
    		<field name="arch" type="xml">
    			<form>
	    			<sheet>
	    				<group>
	    					<field name="name"/>
	    				</group>
	    			</sheet>
    			</form>
    		</field>
    	</record>
    
    	<record model="ir.ui.view" id="service_tree_view">
    		<field name="name">Dich vu</field>
    		<field name="model">phaply.service</field>
    		<field name="arch" type="xml">
    			<tree>
    				<field name="name"/>
    			</tree>
    		</field>
    	</record>
    	
    	<record model="ir.actions.act_window" id="service_action">
    		<field name="name">Dich vu</field>
    		<field name="res_model">phaply.service</field>
    		<field name="view_mode">tree,form</field>
    	</record>
    	
        <menuitem id="service_menu" name="Dich vu"
                  parent="phaply_config_menu"
                  action="service_action"/>
    </data>
</openerp>